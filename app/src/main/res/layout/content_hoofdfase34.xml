<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.hsl.imtpmd.imtpmd.Hoofdfase34"
    tools:showIn="@layout/activity_hoofdfase34"
    android:orientation="vertical">

    <Button
        android:id="@+id/puntenButton34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:backgroundTint="#3f51b5"
        android:text="Punten overzicht"
        android:textAlignment="center"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/HoofdH23Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hoofdvakken hoofdfase 2 3"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp" />

    <ListView
        android:id="@+id/hoofdvakkenh23"
        android:layout_width="match_parent"
        android:layout_height="128dp" />

    <TextView
        android:id="@+id/KeuzeH23Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Specialisatie hoofdfase 2 3"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp" />

    <ListView
        android:id="@+id/KeuzeH23"
        android:layout_width="match_parent"
        android:layout_height="127dp" />

    <TextView
        android:id="@+id/SpecialisatieH23Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Keuze hoofdfase 2 3"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp" />

    <ListView
        android:id="@+id/SpecialisatieH23"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
